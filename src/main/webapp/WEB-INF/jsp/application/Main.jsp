							$(".compony-news ul").append(
									'<li><a href="${pageContext.request.contextPath}/ofly/ep/main/goNewsInfo?id='
											+ item.id + '&type=0" title="'
											+ item.title + '" class="item">'
											+ item.title + '</a></li>');

						});
					}
				}
			},
			error : function() {

			}
		});
